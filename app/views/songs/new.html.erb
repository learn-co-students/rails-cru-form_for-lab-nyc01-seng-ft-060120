<h1>Create a Song</h1>

<%= form_tag songs_path do %>
  <p><%= label_tag 'Song Name:'%></p>
  <p><%= text_field_tag :'song[name]' %></p>
  <p><%= label_tag 'Genre ID:'%></p>
  <p><%= text_field_tag :'song[genre_id]' %></p>
  <p><%= label_tag 'Artist ID:'%></p>
  <p><%= text_field_tag :'song[artist_id]' %></p>
  <p><%= submit_tag 'Create Song' %></p>
<% end %>