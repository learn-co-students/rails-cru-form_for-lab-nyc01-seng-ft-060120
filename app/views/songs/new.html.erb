<h1>Song Form</h1>

<p><%= form_for @song do |f|%></p>
<p><%= f.label :name%></p>
<p><%= f.text_field :name%></p>

<p><%= f.label :genre_id, "Genre"%></p>
<p><%= f.collection_select :genre_id, Genre.all, :id, :name%></p>

<p><%= f.label :artist_id, "Artist"%></p>
<p><%= f.collection_select :artist_id, Artist.all, :id, :name%></p>

<p><%= f.submit%></p>
<%end%>
